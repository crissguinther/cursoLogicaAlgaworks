
public class IteracaoFor {

	public static void main(String[] args) {

		Double[] carrinhoDeCompras = new Double[]{
				50.0,
				50.0,
				50.0
				};
		
		Double total = 0.0;
		for(int i = 0; i < carrinhoDeCompras.length; i++) {
			total += carrinhoDeCompras[i];
		}
		
		System.out.println(total);
		
	}

}
